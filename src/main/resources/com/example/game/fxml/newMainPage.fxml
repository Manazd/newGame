<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>


<BorderPane prefHeight="480.0" prefWidth="820.0" style="-fx-background-color: #ACE5FF;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.game.Views.MainPageController">
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="profile" layoutX="57.0" layoutY="272.0" mnemonicParsing="false" onAction="#showProfile" prefHeight="37.0" prefWidth="82.0" text="Profile">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="newGame" layoutX="191.0" layoutY="272.0" mnemonicParsing="false" onAction="#newGame" prefHeight="40.0" prefWidth="115.0" text="New game">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="exit" layoutX="655.0" layoutY="272.0" mnemonicParsing="false" onAction="#exit" prefHeight="37.0" prefWidth="82.0" text="Exit">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="scores" layoutX="348.0" layoutY="272.0" mnemonicParsing="false" onAction="#showScores" prefHeight="37.0" prefWidth="82.0" text="Scores">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="setting" layoutX="482.0" layoutY="272.0" mnemonicParsing="false" onAction="#showSetting" prefHeight="40.0" prefWidth="99.0" text="Setting">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
